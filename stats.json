{
  "lastUpdated": "2026-01-29T12:00:00Z",
  "config": {
    "birthDate": "1996-07-15",
    "yearStart": "2026-01-01",
    "trackingStart": "2026-01-01"
  },
  "stats": [
    {
      "name": "Days Alive",
      "formula": "days_since('birthDate')",
      "format": "integer",
      "description": "Total days since birth"
    },
    {
      "name": "Days This Year",
      "formula": "days_since('yearStart')",
      "format": "integer",
      "description": "Days elapsed in current year"
    },
    {
      "id": "books_read",
      "name": "Books Read",
      "value": 42,
      "description": "Books completed this year"
    },
    {
      "id": "workout_days",
      "name": "Workout Days",
      "value": 150,
      "description": "Days exercised this year"
    },
    {
      "name": "Workout Rate",
      "formula": "(workout_days / days_since('yearStart')) * 100",
      "format": "percentage",
      "description": "Percentage of days worked out"
    },
    {
      "name": "Average Books/Month",
      "formula": "books_read / (days_since('yearStart') / 30.44)",
      "format": "decimal",
      "description": "Monthly reading rate"
    },
    {
      "id": "savings",
      "name": "Savings",
      "value": 5000,
      "description": "Total savings in USD"
    },
    {
      "id": "income",
      "name": "Monthly Income",
      "value": 3000
    },
    {
      "name": "Savings Rate",
      "formula": "(savings / (income * 12)) * 100",
      "format": "percentage",
      "description": "Percentage of yearly income saved"
    },
    {
      "name": "Days Until Year End",
      "formula": "365 - days_since('yearStart')",
      "format": "integer",
      "description": "Days remaining this year"
    }
  ]
}
